bell_msg ""
vbell off
caption always ' [%H] %n %t %=%D %d %M %Y '
startup_message off
defutf8 on
defscrollback 5000
nonblock on
term "screen-256color"


# startup windows

screen -t mutt 1
stuff "mutt\012"

chdir $HOME/git/pim.git/remind
screen -t remind 2
stuff "rem\012"
chdir $HOME

screen -t rss 3
stuff "mutt-rss\012"

screen -t zsh  0
